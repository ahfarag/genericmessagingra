<sun-application-client>
    <resource-ref>
      <res-ref-name>jms/TCFactory</res-ref-name>
      <jndi-name>jms/NOTXTCFactory</jndi-name>
      <default-resource-principal>
        <name>guest</name>
        <password>guest</password>
      </default-resource-principal>
    </resource-ref>
    <resource-ref>
      <res-ref-name>jms/QCFactory</res-ref-name>
